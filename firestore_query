q := client.Collection("regblogs")
	docs, err := q.Documents(context.Background()).GetAll()
	if err != nil {
		// TODO: Handle error.
	}
	for _, doc := range docs {
		//fmt.Println(doc)

		type Blog struct {
			Topic_name    		string  `firestore:"topic_name"`
			Thumbnail 			string 	`firestore:"thumbnail"`
			Short_description	string	`firestore:"short_description"`
			Publish_no			string	`firestore:"publish_no"`
			Html_code			string	`firestore:"html_code"`
		}

		var blog Blog
		if err := doc.DataTo(&blog); err != nil {
			// TODO: Handle error.
		}
		fmt.Println(blog)
